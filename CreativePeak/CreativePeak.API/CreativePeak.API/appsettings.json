{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "Key": "${JWT_KEY}"
  },
  "AWS": {
    "AccessKey": "${AWS_ACCESS_KEY_ID}",
    "SecretKey": "${AWS_SECRET_ACCESS_KEY}",
    "Region": "${AWS_REGION}",
    "BucketName": "${AWS_BUCKET_NAME}"
  },
  "ConnectionStrings": {
    "MyDatabase": "${ConnectionStrings_CreativePeak}"
  },
  "AllowedHosts": "*"
}
